[user]
	email = bivoje49@gmail.com
	name = bivoje
[credential]
	helper = store
[alias]
	ll = log --graph --oneline --all --decorate
	st = status
	amend = commit --amend -C HEAD
[merge]
	tool = vimdiff
[core]
	editor = vim
[color "diff"]
	meta = blue
[ui]
	color = auto
[mergetool "vimdiff"]
	layout = LOCAL,BASE,REMOTE / MERGED + REMOTE,BASE + LOCAL,BASE
